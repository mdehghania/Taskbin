<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/splash">
    <fragment
        android:id="@+id/splash"
        android:name="com.example.taskbin.Fragment.splash"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splash_to_welcome"
            app:destination="@id/welcome" />
    </fragment>
    <fragment
        android:id="@+id/welcome"
        android:name="com.example.taskbin.Fragment.welcome"
        android:label="fragment_welcome"
        tools:layout="@layout/fragment_welcome" >
        <action
            android:id="@+id/action_welcome_to_inputphone"
            app:destination="@id/inputphone" />
    </fragment>
    <fragment
        android:id="@+id/inputphone"
        android:name="com.example.taskbin.Fragment.inputphone"
        android:label="fragment_inputphone"
        tools:layout="@layout/fragment_inputphone" >
        <action
            android:id="@+id/action_inputphone_to_inputUserName"
            app:destination="@id/inputUserName" />
    </fragment>
    <fragment
        android:id="@+id/inputUserName"
        android:name="com.example.taskbin.Fragment.inputUserName"
        android:label="fragment_input_user_name"
        tools:layout="@layout/fragment_input_user_name" >
        <action
            android:id="@+id/action_inputUserName_to_inputPass"
            app:destination="@id/inputPass" />
    </fragment>
    <fragment
        android:id="@+id/inputPass"
        android:name="com.example.taskbin.Fragment.inputPass"
        android:label="fragment_input_pass"
        tools:layout="@layout/fragment_input_pass" />
</navigation>